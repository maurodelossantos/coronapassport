<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cdc.AddDosisCdc">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AÃ±adir dosis a"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/tv_accountAddTest"
        android:layout_width="172dp"
        android:layout_height="169dp"
        android:text="Account: "
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.118" />

    <TextView
        android:id="@+id/tv_nameAddTest"
        android:layout_width="181dp"
        android:layout_height="166dp"
        android:text="Complete Name: "
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toEndOf="@+id/tv_accountAddTest"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.118" />

    <EditText
        android:id="@+id/et_tipoAddTest"
        android:layout_width="366dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_accountAddTest"
        app:layout_constraintVertical_bias="0.068"></EditText>

    <EditText
        android:id="@+id/et_resultadoAddTest"
        android:layout_width="355dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_accountAddTest"
        app:layout_constraintVertical_bias="0.287"></EditText>

    <EditText
        android:id="@+id/et_timestampAddTest"
        android:layout_width="349dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_accountAddTest"
        app:layout_constraintVertical_bias="0.502"></EditText>

    <EditText
        android:id="@+id/et_lugarAddTest"
        android:layout_width="355dp"
        android:layout_height="58dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_accountAddTest"
        app:layout_constraintVertical_bias="0.746"></EditText>

    <Button
        android:id="@+id/bt_addInfoTestCdc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_lugarAddTest" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="37dp"
        android:layout_height="36dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_nameAddTest"
        app:layout_constraintEnd_toEndOf="@+id/tv_nameAddTest"
        app:layout_constraintStart_toStartOf="@+id/tv_nameAddTest"
        app:srcCompat="@drawable/ic_user" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="31dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_accountAddTest"
        app:layout_constraintEnd_toEndOf="@+id/tv_accountAddTest"
        app:layout_constraintStart_toStartOf="@+id/tv_accountAddTest"
        app:srcCompat="@drawable/wallet" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Introduce los datos de la dosis"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_tipoAddTest"
        app:layout_constraintEnd_toEndOf="@+id/et_tipoAddTest"
        app:layout_constraintStart_toStartOf="@+id/et_tipoAddTest"
        app:layout_constraintTop_toBottomOf="@+id/tv_nameAddTest" />
</androidx.constraintlayout.widget.ConstraintLayout>